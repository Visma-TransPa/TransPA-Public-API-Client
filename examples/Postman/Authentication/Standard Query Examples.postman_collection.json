		{
			"name": "Get Access Token",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"var jsonData = JSON.parse(responseBody);\r",
							"console.log(jsonData)\r",
							"console.log(jsonData.access_token)\r",
							"pm.environment.set(\"accessTokenWithVehicleScope\", jsonData.access_token);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"let clientId = pm.environment.get(\"clientId\");\r",
							"let clientSecret = pm.environment.get(\"clientSecret\");\r",
							"\r",
							"const encodedString = Buffer.from(clientId + \":\" + clientSecret).toString('base64');\r",
							"\r",
							"pm.collectionVariables.set(\"encodedCredentials\", encodedString);\r",
							"\r",
							"console.log(\"Base64 Encoded Token:\", encodedString);"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"value": "application/x-www-form-urlencoded",
						"type": "text"
					},
					{
						"key": "Authorization",
						"value": "Basic {{encodedCredentials}}",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "grant_type=client_credentials&scope=transpaapi:api+transpaapi:vehicles:read+transpaapi:trips:read&tenant_id={{tenantId}}",
					"options": {
						"raw": {
							"language": "text"
						}
					}
				},
				"url": {
					"raw": "https://connect.visma.com/connect/token",
					"protocol": "https",
					"host": [
						"connect",
						"visma",
						"com"
					],
					"path": [
						"connect",
						"token"
					]
				}
			},
			"response": []
		}
	],
	"variable": [
		{
			"key": "encodedCredentials",
			"value": ""
